<p style="color: green"><%= notice %></p>
<div class="container">
  <div class="row">
    <div class="col">
      <%= render partial: 'profile', locals: {user: current_user} %>    
    </div>
    <div class="col">
      <%= render 'feed' %>
    </div>
    <div class="col">
      <% unless current_user.friendships.accepted_friend.empty?%>
        <%= render 'friends' %>
      <% end %>
    </div>
  </div>
</div>



